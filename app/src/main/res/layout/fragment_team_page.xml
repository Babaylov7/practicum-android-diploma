<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/background"
    tools:context=".ui.team.TeamPageFragment">

    <TextView
        android:id="@+id/text_team"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/five_space"
        android:text="@string/text_team"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/twenty_two_size_text"
        android:fontFamily="@font/ys_display_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_worked_application"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/eleven_space"
        android:fontFaily="@font/ys_display_bold"
        android:text="@string/text_developer_worker"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/thirty_two_size_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_team" />

    <TextView
        android:id="@+id/text_developer_name_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/eight_space"
        android:text="@string/text_developer_name_first"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/sixteen_size_text"
        android:fontFamily="@font/ys_display_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_worked_application"
        />

    <TextView
        android:id="@+id/text_developer_name_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/four_space"
        android:text="@string/text_developer_name_second"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/sixteen_size_text"
        android:fontFamily="@font/ys_display_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_first" />

    <TextView
        android:id="@+id/text_developer_name_third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/four_space"
        android:text="@string/text_developer_name_third"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/sixteen_size_text"
        android:fontFamily="@font/ys_display_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_second" />

    <TextView
        android:id="@+id/text_developer_name_fourth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/four_space"
        android:text="@string/text_developer_name_four"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/sixteen_size_text"
        android:fontFamily="@font/ys_display_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_third" />

    <TextView
        android:id="@+id/text_developer_name_fifth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/four_space"
        android:layout_marginTop="@dimen/four_space"
        android:text="@string/text_developer_name_five"
        android:textColor="?attr/text_color"
        android:textSize="@dimen/sixteen_size_text"
        android:fontFamily="@font/ys_display_medium"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_fourth" />

</androidx.constraintlayout.widget.ConstraintLayout>
